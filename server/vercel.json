{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT","DELETE","PATCH","OPTIONS"],
        "header": {
          "Access-Control-Allow-Origin":"*"
        }
      },
      {
        "src": "/login",
        "dest": "/login"
      },
      {
        "src": "/signup",
        "dest": "/signup"
      }
    ]
  }
